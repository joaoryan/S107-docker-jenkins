{
  "name": "clean-node",
  "version": "1.2.0",
  "description": "",
  "main": "test.js",
  "scripts": {
    "start": "npx env-cmd -f .env.development node dist/main/server.js",
    "build": "npx rimraf dist && tsc",
    "dev": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "jest --passWithNoTests  --runInBand --watchAll",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:pipeline": "npm test -- -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "clear_jest": "jest --clearCache",
    "prepare": "husky install",
    "homol": "npx env-cmd -f .env.homol node ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mysql": "^2.15.20",
    "@types/node": "^16.11.7",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
